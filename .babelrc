{
  "presets": [
    "@babel/preset-env",
    "@babel/react"
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "emotion",
          {
            "sourceMap": true,
            "autoLabel": true
          }
        ],
        [
          "@babel/plugin-transform-runtime"
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "emotion",
          {
            "hoist": true
          }
        ]
      ]
    }
  }
}
